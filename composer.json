{
    "name": "lakuko/filament-user-sanctum",
    "description": "Filament plugin to manage user and token from sanctum",
    "keywords": [
        "Lakuko",
        "laravel",
        "filament-user-sanctum"
    ],
    "homepage": "https://github.com/lakuko/filament-user-sanctum",
    "license": "MIT",
    "authors": [
        {
            "name": "Lakuko",
            "email": "1040745+dulabs@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.0",
        "spatie/laravel-package-tools": "^1.13.5",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-livewire": "^1.0",
        "pestphp/pest-plugin-parallel": "^0.3",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Lakuko\\FilamentUserSanctum\\": "src",
            "Lakuko\\FilamentUserSanctum\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lakuko\\FilamentUserSanctum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test:pest": "vendor/bin/pest --parallel",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lakuko\\FilamentUserSanctum\\FilamentUserSanctumServiceProvider"
            ],
            "aliases": {
                "FilamentUserSanctum": "Lakuko\\FilamentUserSanctum\\Facades\\FilamentUserSanctum"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}